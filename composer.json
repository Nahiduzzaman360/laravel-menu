{
    "name": "bdhabib/laravel-menu",
    "description": "Drag & Drop menu builder like  WordPress for Laravel 12.x",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "license": "MIT",
    "version": "1.1",
    "authors": [
        {
            "name": ":Habibur Rahaman",
            "email": "shamolrahaman@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.13.0"
    },
    "autoload": {
        "psr-4": {
            "Bdhabib\\LaravelMenu\\": "src/",
            "Bdhabib\\LaravelMenu\\Database\\Factories\\": "database/factories/"
        }
    },

    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bdhabib\\LaravelMenu\\LaravelMenuServiceProvider"
            ],
            "aliases": {
                "LaravelMenu": "Bdhabib\\LaravelMenu\\Facades\\LaravelMenu"
            }
        }
    },
    "prefer-stable": true
}
